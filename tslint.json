{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single", "jsx-double"],
      "interface-name": [false],
      "jsx-no-multiline-js": [false],
      "ordered-imports": [true],
      "arrow-parens": [false],
      "max-line-length": false
    },
    "rulesDirectory": []
}
